module sparkbot-hello-world {
    yang-version 1;
    namespace "urn:opendaylight:params:xml:ns:yang:sparkbot:application:helloworld";
    prefix "hwapp";

    rpc run {
        description
            "Performs the run of the 'Hello World' application";
        input {
            leaf access-token {
                type string;
                mandatory true;
                description
                    "Optional access token that overrides the default
                    access token in the config data store.";
            }
            leaf webhook-http-port {
                type uint32;
                description
                    "Optional port number that overrides the default port
                    number for the Sparkbot HTTP server.";
            }
        }
    }


}